#include <stdio.h>
#include <stdlib.h>

int main() {
	system("CLS");
	
	int num1;
	int num2;
	int resultado;
	
	printf("Ingrese un numero:\n");
	scanf("%d",&num1);
	printf("Ingrese un numero:\n");
	scanf("%d",&num2);
	
	resultado = (num1-num1);
	if(resultado > 0) {
	printf("Resultado positivo");
	} else {
	printf("Resultado negativo");
	}
}
